<!-- CREDIT : PLS DONT DELETE! -->

<a href="https://sprfluous.tumblr.com" title="code by anna"><div class="ananas"><i class="far fa-star"></i></div></a>

<script>
    
function timeChange(){
    
    var d = new Date();
    
    var a = '<a href="/">';
    
    var x = d.getHours();
    var y = d.getMinutes();
    
    
    if (x > 12){
        x = x-12;
    }
    
    if (y < 10){
        y = '0'+y;
    }
    
    a += x;
    a += ':';
    a += y;
    a += '</a>';
    
    document.getElementById('time').innerHTML = a;
    
    var w = d.getDate();
    var v = d.getFullYear();
    
    var months = ['january', 'february', 'march', 'april', 'may', 'june', 'july', 'august', 'september', 'october', 'november', 'december'];
    
    var z = months[d.getMonth()];
    
    var b = '';
    
    b += w;
    b += ' ';
    b += z;
    b += ' ';
    b += v;
    
    document.getElementById('date').innerHTML = b;
    
}

window.onload = timeChange;

</script>

</body>
</html>
