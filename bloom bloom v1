<script>

function revealContents() {
    window.setTimeout(fadeout, 100);
    window.setTimeout(destroy, 500);
}

function fadeout() {
    document.getElementById('hugeimage').classList.add('die');
}

function destroy() {
    document.getElementById('hugeimage').style.display = "none";
}

function meetMuses() {
    goBack1();
    window.setTimeout(musesfade, 100);
    window.setTimeout(destroy1, 500);
}

function meetMun() {
    goBack();
    window.setTimeout(munfade, 100);
    window.setTimeout(destroy2, 500);
}

function goBack() {
    document.getElementById('muses').style.display = "none";
    document.getElementById('museshead').classList.remove('die');
    document.getElementById('museshead').style.display = "block";
}

function goBack1() {
    document.getElementById('mun').style.display = "none";
    document.getElementById('munhead').classList.remove('die');
    document.getElementById('munhead').style.display = "block";
}

function musesfade() {
    document.getElementById('museshead').classList.add('die');
    document.getElementById('muses').style.display = "block";
}

function destroy1() {
    document.getElementById('museshead').style.display = "none";
}

function munfade() {
    document.getElementById('munhead').classList.add('die');
    document.getElementById('mun').style.display = "block";
}

function destroy2() {
    document.getElementById('munhead').style.display = "none";
}

function openMuse(evt, museNumber) {
    var i, musebox, muselinks;
    musebox = document.getElementsByClassName("musebox");
    for (i = 0; i < musebox.length; i++) {
        musebox[i].style.display = "none";
    }
    muselinks = document.getElementsByClassName("muse");
    for (i = 0; i < muselinks.length; i++) {
        muselinks[i].className = muselinks[i].className.replace(" active", "");
    }
    document.getElementById(museNumber).style.display = "block";
    evt.currentTarget.className += " active";
}


</script>



<div id="container">

