    <li><span><b>markings :</b></span>
        <span><i>none</i></span>
    </li>
    <li><span><b>piercings :</b></span>
        <span><i>earlobes</i></span>
    </li>
    <li><span><b>tattoos :</b></span>
        <span><i>none (yet)</i></span>
    </li>
    <li><span><b>face claim :</b></span>
        <span><i>heo hyunjoon</i></span>
    </li>
</ul>

</div>
