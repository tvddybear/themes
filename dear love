<script>

function openTab(tabname){
    var x = document.getElementsByClassName('tab');
    for (i = 0; i < x.length; i++){
        x[i].style.display = 'none';
    }
    document.getElementById(tabname).style.display = 'block';
    document.getElementById('sidebar').style.animation = 'fadeOut .5s';
    document.getElementById('sidebar').style.display = 'none';
    setTimeout(function () {
        document.getElementById('sidebar').style.animation = 'fadeIn .5s';
    }, 500);
}

function closeTab(element){
    document.getElementById('sidebar').style.display = 'block';
    element.style.animation = 'fadeOut .5s';
    element.style.display = 'none';
    setTimeout(function () {
        element.style.animation = 'fadeIn .5s';
    }, 500);
}  

function openPopup(popupname){
    document.getElementById(popupname).style.display = 'block';
    var x = document.getElementById('navi');
    x.style.animation = 'fadeOut .5s';
    x.style.display = 'none';
    setTimeout(function () {
        x.style.animation = 'fadeIn .5s';
    }, 500);
}

function closePopup(element){
    element.style.animation = 'fadeOut .5s';
    element.style.display = 'none';
    document.getElementById('navi').style.display = 'block';
    setTimeout(function () {
        element.style.animation = 'fadeIn .5s';
    }, 500);
}
    
</script>

