<!-- CREDIT : PLS DONT DELETE! -->

    <div class="ananas"><a href="https://heochanz.tumblr.com" title="code by anna"><span class="th th-pineapple-o"></span></a></div>

<script>

function timeChange(){
    var d = new Date();
    var a = '';
    var x = d.getHours();
    var y = d.getMinutes();
    if (x > 12){
        x = x-12;
    }
    if (y < 10){
        y = '0'+y;
    }
    a += x;
    a += ':';
    a += y;
    document.getElementById('phonetime').innerHTML = a;
   
    var w = d.getDate();
    var days = ['sunday','monday','tuesday','wednesday','thursday','friday','saturday'];
    var v = days[d.getDay()];
    var months = ['january', 'february', 'march', 'april', 'may', 'june', 'july', 'august', 'september', 'october', 'november', 'december'];
    var z = months[d.getMonth()];
    var b = '';
    b += v;
    b += ', ';
    b += w;
    b += ' ';
    b += z;
    document.getElementById('phonedate').innerHTML = b;
}
 
window.onload = timeChange;
    
</script>

</body>
</html>
