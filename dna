<script>
    
window.onload = function(){
    var a = document.getElementsByClassName('tlink');
    for (i = 0; i < a.length; i++) {
        var x = ["{color:accent}","{color:accent 2}","{color:italic}"];
        var y = [Math.floor(Math.random() * x.length)];
        a[i].style.backgroundColor = x[y];
    }
}
    
</script>

