<script>

function openNav(){
    $("#one").show();
    $(".posts").hide();
}

function closeNav(){
    $('#one').hide();
    $('.posts').show();
}

function openPopup(id){
    var x = '#popup'
    x += id
    $(x).show();
}
 
function closePopup(id){
    var x = '#popup'
    x += id
    $(x).hide();
}
    
</script>

