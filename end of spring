    <div class="ananas"><a href="https://heochanz.tumblr.com" title="code by anna"><span class="th th-pineapple-o"></span></a></div>

<script>

q = 0;
    
document.getElementById('audiosong').onended = function(){
    var a = document.getElementById('audiosong');
    var temp = a.src;
    a.src = document.getElementById('audiosong1').src;
    document.getElementById('audiosong1').src = document.getElementById('audiosong2').src;
    document.getElementById('audiosong2').src = document.getElementById('audiosong3').src;
    document.getElementById('audiosong3').src = document.getElementById('audiosong4').src;
    document.getElementById('audiosong4').src = temp;
    a.play();
    a.volume = 0.1;
    q = q + 1;
    if (q === 5) {
        q = 0;
    }
    var d = document.getElementsByClassName('songname')[q - 1];
    var e = document.getElementsByClassName('artistname')[q - 1];
    d.style.display = 'none';
    e.style.display = 'none';
    document.getElementsByClassName('songname')[q].style.display = 'block';
    document.getElementsByClassName('artistname')[q].style.display = 'block';
}
    
</script>

</body>
</html>
