<script>

function audioPlay(){
    var a = document.getElementById('audiosong');
    a.loop = false;
    var b = document.getElementsByClassName('replacethis');
    if (a.paused){
        a.play();
        a.volume = 0.1;
        for (i = 0; i < b.length; i++){
            $(b[i]).removeClass('fa-play');
            $(b[i]).addClass('fa-pause');
        }
    } else {
        a.pause();
        for (i = 0; i < b.length; i++){
            $(b[i]).removeClass('fa-pause');
            $(b[i]).addClass('fa-play');
        }
    }
}

function openTab(tabname){
    document.getElementsByClassName('posts')[0].style.animation = 'fadeOut .5s';
    setTimeout(function () {
        document.getElementsByClassName('posts')[0].style.display = 'none';
    }, 500);
    var x = document.getElementsByClassName('tab');
    for (i = 0; i < x.length; i++){
        x[i].style.display = 'none';
    }
    document.getElementById(tabname).style.display = 'block';
}

function closeTab(element){
    document.getElementsByClassName('posts')[0].style.animation = 'fadeIn .5s';
    document.getElementsByClassName('posts')[0].style.display = 'block';
    element.style.animation = 'fadeOut .5s';
    setTimeout(function () {
        element.style.display = 'none';
        element.style.animation = 'fadeIn .5s';
    }, 500);
}

</script>

