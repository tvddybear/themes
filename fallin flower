<script>
    
function openTab(tabname){
    document.getElementsByClassName('posts')[0].style.display = 'none';
    var x = document.getElementsByClassName('tab');
    for (i = 0; i < x.length; i++) {
        x[i].style.display = 'none';
    }
    document.getElementById(tabname).style.display = 'block';
}

function audioPlay(){
    var a = document.getElementById('audiosong');
    var b = document.getElementById('replacethis');
    if (a.paused){
        a.play();
        a.volume = 0.3;
        $(b).removeClass('fa-play');
        $(b).addClass('fa-pause');
    } else {
        a.pause();
        $(b).removeClass('fa-pause');
        $(b).addClass('fa-play');
    }
}
    
</script>

