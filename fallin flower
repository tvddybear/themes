    <div id="dot"></div>
    
    <a href="/"><div class="navlink"><span class="fas fa-home"></span></div></a>
    <a href="/ask"><div class="navlink"><span class="fas fa-envelope-open"></span></div></a>
    <a onclick="openTab('navi')"><div class="navlink"><span class="fas fa-globe-asia"></span></div></a>
    <a onclick="openTab('muses')"><div class="navlink"><span class="fas fa-star"></span></div></a>
    
    <div id="navtitle">navigate.</div>
    
</div>

<div id="description">
    
    {text:your description}
    
</div>

<!-- AUDIO PLAYER -->

<div id="audiobox">

    <div id="audioplay">&nbsp;<span id="replacethis" class="fas fa-play" onclick="audioPlay();"></span></div>
    
    <div id="songname">{text:song title}</div>
    <div id="artistname">{text:song artist}</div>
    
</div>

<audio id="audiosong" src="{text:link to audio}"></audio>
<!-- END AUDIO PLAYER -->

<div id="navi" class="tab">
    
    <div class="dot" onclick="this.parentElement.style.display = 'none';document.getElementsByClassName('posts')[0].style.display = 'block';"></div>
    
    <div id="navimg"><img src="{image:nav tab img}"/></div>
    
    <div id="navtab">
        
