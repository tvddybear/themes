<script>
   
function openTab(tabid){
    document.getElementsByClassName('tab')[0].style.display = 'block';
    document.getElementsByClassName('posts')[0].style.display = 'none';
}

function closeTab(){
    document.getElementsByClassName('posts')[0].style.display = 'block';
    document.getElementsByClassName('tab')[0].style.display = 'none';
}
 
</script>

