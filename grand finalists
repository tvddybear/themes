<div class="post">

<!-- TEXT POSTS -->
{block:Text}
{block:Title}<h1>{Title}</h1>{/block:Title}
{Body}
{/block:Text}
 
<!-- ANSWER POSTS -->
{block:Answer}
<div class="askee"><i> {Asker} asked:</i></div><div class="question">{Question}
</div>
{Answer}
{/block:Answer}
 
<!-- QUOTE POSTS -->
{block:Quote}
<div class="quotetext">“ {Quote} ”
{block:Source}<br><div class="source">— <i>{Source}</i></div>{/block:Source}
</div>
{/block:Quote}
 
<!-- LINK POSTS -->
{block:Link}
<link><a href="{URL}">{Name}</a></link>
{block:Description}{Description}{/block:Description}    
{/block:Link}
 
<!-- CHAT POSTS -->
{block:Chat}
{block:Title}<h2>{Title}</h2>{/block:Title}
{block:Lines}
{block:Label}<b>{Label}</b>{/block:Label}
{Line}<br>
{/block:Lines}
{/block:Chat}
<!-- PHOTO POSTS -->
{block:Photo}
{LinkOpenTag}<img src="{PhotoURL-500}" alt="{PhotoAlt}" width="400px">{LinkCloseTag}
 
{block:Caption}{Caption}{/block:Caption}
{/block:Photo}
 
<!-- PHOTOSET POSTS -->
{block:Photoset}
{Photoset}
{block:Caption}{Caption}{/block:Caption}
{/block:Photoset}

<!-- AUDIO POSTS -->
 
