<script>

var $grid = $('.grid').masonry({
  columnWidth: 400, // if you change the width of the boxes you NEED to change this as well so that it still looks good.
  isFitWidth: true
});

function selectAll(){
    var x = document.getElementsByClassName('grid-item');
    for (i = 0; i < x.length; i += 1) {
        $(".grid-item").removeClass("hidden");
        $('.grid').masonry({
  itemSelector: '.grid-item',
  columnWidth: 300, // dont worry about this masonry is a lil b*tch
  isFitWidth: true
});
$('.grid').masonry({
  itemSelector: '.grid-item',
  columnWidth: 400, // match with the one above.
  isFitWidth: true
});
    }
}

function selectCat(category){
    
    var x = document.getElementsByClassName('grid-item');
    var y = document.getElementsByClassName(category);
    var category1 = '.'
    category1 += category
    for (i = 0; i < x.length; i += 1) {
        $(".grid-item").addClass("hidden");
    }
    for (i = 0; i < y.length; i += 1) {
        $(category1).removeClass("hidden");
        $('.grid').masonry({
  itemSelector: '.grid-item',
  columnWidth: 300, // again, don't worry, it's just me fighting the system to get the functionality we all want.
  isFitWidth: true
});
$('.grid').masonry({
  itemSelector: '.grid-item',
  columnWidth: 400, // match with the ones above
  isFitWidth: true
});
    }
}

function readMore(category){
    var x = document.getElementById(category);
    x.style.display = 'block';
}

</script>

<div class="header">
    <!-- TITLE -->
    <h1>hope world</h1>
    
    <!-- FILTERS -->
    <div class="filters">
        <b>CATEGORIES :</b><br>
        
        <!-- A NOTE ON EDITING THE FILTERS:
        
        if you're changing the categories, change the thing inside selectCat (e.g. if you're changing geography to... idk, climate, you would change it from selectCat('geography') to selectCat('climate'). whatever you put inside the selectCat becomes the class of the boxes associated with that category (e.g. below the box associated with geography has class="grid-item geography").
        
        -->
        
        <span onclick="selectAll()">all</span> &nbsp; 
        <span onclick="selectCat('geography')">geography</span> &nbsp; 
        <span onclick="selectCat('history')">history</span> &nbsp; 
        <span onclick="selectCat('culture')">culture</span> &nbsp; 
        <span onclick="selectCat('flora')">flora</span> &nbsp; 
        <span onclick="selectCat('fauna')">fauna</span> &nbsp; 
        <span onclick="selectCat('industry')">industries</span> &nbsp; 
        <span onclick="selectCat('education')">education</span> &nbsp; 
        <span onclick="selectCat('religion')">religion</span> &nbsp; 
        <span onclick="selectCat('politics')">politics</span> &nbsp; 
        <span onclick="selectCat('fashion')">fashion</span>
    </div>
</div>

