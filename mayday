<script>

function timeChange(){
    var d = new Date();
    var a = '';
    var x = d.getHours();
    var y = d.getMinutes();
    if (x > 12){
        x = x-12;
    }
    if (y < 10){
        y = '0'+y;
    }
    a += x;
    a += ':';
    a += y;
    document.getElementById('phonetime').innerHTML = a;
   
    var w = d.getDate();
    var days = ['sunday','monday','tuesday','wednesday','thursday','friday','saturday'];
    var v = days[d.getDay()];
    var months = ['january', 'february', 'march', 'april', 'may', 'june', 'july', 'august', 'september', 'october', 'november', 'december'];
    var z = months[d.getMonth()];
    var b = '';
    b += v;
    b += ', ';
    b += w;
    b += ' ';
    b += z;
    document.getElementById('phonedate').innerHTML = b;
    
    var xyz = document.getElementsByClassName('link');
    var abc = ["{color:accent}","{color:accent 2}","{color:italic}"];
    for (pqr = 0; pqr < xyz.length; pqr++){
        var zef = [Math.floor(Math.random() * abc.length)];
        xyz[pqr].style.borderColor = abc[zef];
    }
}
 
window.onload = timeChange;
    
</script>

</body>
</html>
