<script>

function openTab(tabname){
    var a = document.getElementById('posts');
    a.style.animation = 'fadeOut .5s';
    a.style.display = 'none';
    var x = document.getElementsByClassName('tab');
    for (i = 0; i < x.length; i++){
        x[i].style.display = 'none';
    }
    document.getElementById(tabname).style.display = 'block';
    setTimeout(function(){
        a.style.animation = 'fadeIn .5s';
    }, 500);
}

function closeTab(element){
    element.style.animation = 'fadeOut .5s';
    document.getElementById('posts').style.display = 'block';
    setTimeout(function () {
        element.style.display = 'none';
        element.style.animation = 'fadeIn .5s';
    }, 500);
}

function openPopup(popupname){
    var x = document.getElementsByClassName('popup');
    for (i = 0; i < x.length; i++){
        x[i].style.display = 'none';
    }
    document.getElementById(popupname).style.display = 'block';
}

</script>

