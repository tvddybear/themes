<script>
    
window.onload = function(){
    var x = document.getElementById("sidebartitle");
    var y = x.style.fontFamily;
    if (y = 'himagsikan'){
        x.style.fontSize = '60px';
        x.style.top = '20px';
    }
}
    
</script>

