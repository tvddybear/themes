<script>
  
function openTab(){
    $("#one").show();
    $('#two').hide();
    $(".posts").hide();
}

function closeTab(){
    $('#one').hide();
    $('#two').hide();
    $('.posts').show();
}

function openNav(){
    $("#two").show();
    $("#one").hide();
    $(".posts").hide();
}

function closeNav(){
    $('#two').hide();
    $("#one").hide();
    $('.posts').show();
}

function openPopup(id){
    var x = '#popup'
    x += id
    $(x).show();
}

function closePopup(id){
    var x = '#popup'
    x += id
    $(x).hide();
}
    
</script>

