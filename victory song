<script>
  
function openTab(){
    $("#one").show();
    $('#two').hide();
    $(".posts").hide();
}

function closeTab(){
    $('#one').hide();
    $('#two').hide();
    $('.posts').show();
}

function openNav(){
    $("#two").show();
    $("#one").hide();
    $(".posts").hide();
}

function closeNav(){
    $('#two').hide();
    $("#one").hide();
    $('.posts').show();
}
</script>

